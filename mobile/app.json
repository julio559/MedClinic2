{
  "expo": {
    "name": "Medical AI",
    "slug": "medical-ai-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",

    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": ["**/*"],

    "runtimeVersion": { "policy": "sdkVersion" },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1E3A8A"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
